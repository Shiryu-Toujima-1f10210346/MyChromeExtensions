{
    "manifest_version": 3,
    "name": "いろいろ拡張機能",
    "author": "Shiryu",
    "version": "1.0.0",
    "description": "なんでもできる拡張機能",
    "action": {
      "default_popup": "src/popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://github.com/*",
          "<all_urls>"
        ],
        "js": [
          "src/github.js",
          "src/X2Twitter.js"
        ]
      }
    ]
  }